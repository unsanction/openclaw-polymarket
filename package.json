{
  "name": "@c0pilot/openclaw-polymarket",
  "version": "1.0.0",
  "description": "OpenClaw plugin for Polymarket prediction markets - browse markets, place orders, manage positions",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openclaw",
    "polymarket",
    "prediction-markets",
    "trading",
    "crypto",
    "polygon",
    "ai-agent"
  ],
  "author": "c0pilot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/c0pilot/openclaw-polymarket"
  },
  "homepage": "https://github.com/c0pilot/openclaw-polymarket#readme",
  "bugs": {
    "url": "https://github.com/c0pilot/openclaw-polymarket/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@c0pilot/mcp-polymarket": "^1.0.0",
    "@polymarket/clob-client": "^5.2.1",
    "@sinclair/typebox": "^0.32.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  }
}
